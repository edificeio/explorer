{
  "name": "ode-explorer",
  "version": "1.4.2",
  "description": "Edifice Explorer",
  "homepage": "https://github.com/edificeio/explorer#readme",
  "bugs": {
    "url": "https://github.com/edificeio/explorer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edificeio/explorer.git"
  },
  "license": "AGPL-3.0",
  "author": "Edifice",
  "type": "module",
  "exports": {
    "./lib": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "dist",
    "lib",
    "*.d.ts"
  ],
  "scripts": {
    "build": "vite build && vite build --mode lib",
    "clean": "concurrently \"pnpm:clean:*\"",
    "clean:dist": "rm -rf dist",
    "clean:lighthouse": "rm -rf .lighthouseci",
    "clean:modules": "rm -rf node_modules",
    "clean:package": "rm -f pnpm-lock.yaml && rm -f package.json",
    "dev": "vite",
    "fix": "eslint --fix .",
    "format": "pnpm format:write && pnpm format:check",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lighthouse": "pnpm clean:lighthouse && lhci autorun",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky frontend/.husky",
    "preview": "vite preview",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm format",
      "pnpm fix"
    ]
  },
  "dependencies": {
    "@edifice-ui/icons": "develop",
    "@edifice-ui/react": "develop",
    "@react-spring/web": "9.7.3",
    "@tanstack/react-query": "5.28.8",
    "clsx": "2.1.0",
    "dayjs": "1.11.10",
    "i18next": "23.8.1",
    "i18next-http-backend": "2.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.12",
    "react-hook-form": "7.49.3",
    "react-i18next": "14.1.0",
    "react-intersection-observer": "9.5.3",
    "zustand": "4.5.0"
  },
  "devDependencies": {
    "@axe-core/react": "4.8.4",
    "@babel/plugin-transform-react-pure-annotations": "7.23.3",
    "@tanstack/react-query-devtools": "5.28.8",
    "@types/node": "20.11.10",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitejs/plugin-react": "4.2.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "concurrently": "8.2.2",
    "csstype": "3.1.3",
    "edifice-bootstrap": "develop",
    "edifice-ts-client": "develop",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "husky": "9.0.7",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-plugin-dts": "3.7.2",
    "vite-tsconfig-paths": "4.3.1"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": "18 || 20"
  }
}