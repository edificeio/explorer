{
  "name": "ode-explorer",
  "version": "1.0.0-dev.20230671116",
  "description": "Open Digital Education Explorer",
  "homepage": "https://github.com/opendigitaleducation/explorer#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/explorer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/explorer.git"
  },
  "license": "GPL-3.0",
  "author": "Open Digital Education",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "clean": "concurrently \"pnpm:clean:*\"",
    "clean:dist": "rm -rf dist",
    "clean:lighthouse": "rm -rf .lighthouseci",
    "clean:modules": "rm -rf node_modules",
    "clean:package": "rm -f pnpm-lock.yaml && rm -f package.json",
    "dev": "vite",
    "fix": "eslint --fix .",
    "format": "pnpm format:check && pnpm format:write",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lighthouse": "pnpm clean:lighthouse && lhci autorun",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run fix"
    ]
  },
  "dependencies": {
    "@ode-react-ui/components": "dev",
    "@ode-react-ui/core": "dev",
    "@ode-react-ui/hooks": "dev",
    "@ode-react-ui/icons": "dev",
    "@react-spring/web": "9.7.2",
    "@tanstack/react-query": "4.29.12",
    "dayjs": "1.11.8",
    "i18next": "23.2.6",
    "i18next-http-backend": "2.2.1",
    "ode-ts-client": "dev",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.9",
    "react-hook-form": "7.44.3",
    "react-hot-toast": "2.4.1",
    "react-i18next": "13.0.1",
    "react-intersection-observer": "9.4.4",
    "swiper": "10.0.3",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@axe-core/react": "4.7.2",
    "@tanstack/react-query-devtools": "4.29.12",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "@vitejs/plugin-react": "4.0.1",
    "clsx": "1.2.1",
    "concurrently": "8.1.0",
    "csstype": "3.1.2",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "5.0.2",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0"
  },
  "packageManager": "pnpm@8.5.0",
  "engines": {
    "node": "18.x"
  }
}
